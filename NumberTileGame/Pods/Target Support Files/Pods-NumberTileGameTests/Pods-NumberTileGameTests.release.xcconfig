FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/lightstep" "${PODS_CONFIGURATION_BUILD_DIR}/opentracing"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/lightstep/lightstep.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/opentracing/opentracing.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "lightstep" -framework "opentracing"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
